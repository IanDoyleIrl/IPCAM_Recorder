<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
    <display-name>Network Camera Monitoring System</display-name>
    <listener>
        <listener-class>org.test.cameraMonitor.recordingEngine.RecordingThreadManager</listener-class>
    </listener>

    <servlet>
        <servlet-name>Test</servlet-name>
        <servlet-class>org.test.cameraMonitor.streamingServer.StreamingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/Test</url-pattern>
    </servlet-mapping>
</web-app>